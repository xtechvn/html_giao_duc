ound: #fff;\r\n  width: 100%;\r\n  font-size: 14px;\r\n  height: 30px;\r\n  line-height: 30px;\r\n  border: 1px solid #ccc;\r\n  padding: 0 5px;\r\n  outline: 0;\r\n  box-shadow: none;\r\n\r\n  &:focus {\r\n    outline: 0;\r\n    box-shadow: none;\r\n  }\r\n}\r\n\r\n.form-control {\r\n  font-size: 14px;\r\n}\r\n\r\n.no-border-input {\r\n  .form-control {\r\n    border: 0;\r\n    border-bottom: 1px solid #ccc;\r\n    border-radius: 0;\r\n    padding-left: 0;\r\n    font-size: 13px;\r\n  }\r\n\r\n  tbody+tbody {\r\n    border-top: 1px solid #dee2e6;\r\n  }\r\n\r\n  th {\r\n    padding-top: 0 !important;\r\n  }\r\n\r\n  td,\r\n  th {\r\n    border-top: 0;\r\n    font-size: 13px;\r\n  }\r\n\r\n  td {\r\n    padding: 5px 10px !important;\r\n  }\r\n}\r\n\r\n.expand {\r\n  display: none;\r\n}\r\n\r\n::-webkit-input-placeholder {\r\n  /* Chrome/Opera/Safari */\r\n  color: #999;\r\n}\r\n\r\n::-moz-placeholder {\r\n  /* Firefox 19+ */\r\n  color: #999;\r\n}\r\n\r\n:-ms-input-placeholder {\r\n  /* IE 10+ */\r\n  color: #999;\r\n}\r\n\r\n:-moz-placeholder {\r\n  /* Firefox 18- */\r\n  color: #999;\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n  content: '';\r\n  display: block;\r\n  height: 0;\r\n  overflow: hidden;\r\n}\r\n\r\n.clearfix:after {\r\n  clear: both;\r\n}\r\n\r\n.left {\r\n  float: left;\r\n}\r\n\r\n.block {\r\n  display: block;\r\n}\r\n\r\n.no-block {\r\n  display: inline-block !important;\r\n}\r\n\r\n.uppercase {\r\n  text-transform: uppercase;\r\n}\r\n\r\n.right {\r\n  float: right;\r\n}\r\n\r\n.relative {\r\n  position: relative;\r\n}\r\n\r\n.align-left {\r\n  text-align: left\r\n}\r\n\r\n.align-right {\r\n  text-align: right\r\n}\r\n\r\n.align-center {\r\n  align-items: center;\r\n}\r\n\r\n.justify-between {\r\n  justify-content: space-between;\r\n}\r\n\r\n.width_common {\r\n  width: 100%;\r\n  float: left;\r\n}\r\n\r\n.container {\r\n  width: 100%;\r\n  max-width: 1600px;\r\n  position: relative;\r\n  clear: both;\r\n}\r\n\r\n\r\nhtml {\r\n  -webkit-t